<script type="text/javascript">
function searchPost() {
    var v_useForm = 0;//用工形式
    var v_trademark = 0;//品牌
    var v_kw = "";//关键字
    var v_pn = "";//职位名称
    var v_p = "";//地点
    var v_pt = 0;//职位类型
    var v_t = 0;//招聘时间
    var v_recruitType = 0;//招聘类型
    var lanType = 0;//lanuage
    var brandCode = "1";//网站品牌
    var v_cp = "";
    var v_scp = "";
    if (null != document.getElementById("useFormV")) {
        v_useForm = document.getElementById("useFormV").value;//用工形式
    }
    if (null != document.getElementById("tradeMarkV")) {
        v_trademark = document.getElementById("tradeMarkV").value;//品牌
    }
    if (null != document.getElementById("keyWordV")) {
        if (document.getElementById("keyWordV").value != document.getElementById("keyWordV").title) {
            v_kw = document.getElementById("keyWordV").value;//关键字
        }
    }
    if (null != document.getElementById("positionNameV")) {
        if (document.getElementById("positionNameV").value != document.getElementById("positionNameV").title) {
            v_pn = document.getElementById("positionNameV").value;//职位名称
        }
    }
    if (null != document.getElementById("workPlaceNameV")) {
        if (document.getElementById("workPlaceNameV").value != document.getElementById("workPlaceNameV").title) {
            v_p = document.getElementById("workPlaceNameV").value;//地点
        }
    }

    if (null != document.getElementById("positionTypeV") ) {
        v_pt = document.getElementById("positionTypeV").value;//职位类型
    }
    if (null != document.getElementById("comPartV")) {
        v_cp = document.getElementById("comPartV").value;//公司部门
    }
    if (null != document.getElementById("sicCorpCodeV")) {
    	v_scp = document.getElementById("sicCorpCodeV").value;//corpCode企业名
    }
    if (null != document.getElementById("dateV")) {
        v_t = document.getElementById("dateV").value;//招聘时间
    }
    //v_recruitType = document.getElementById("recruitTypeV").value;//招聘类型
    v_recruitType = document.getElementById("recruitType").value;//招聘类型
    if (typeof (v_recruitType) == "undefined") {
        v_recruitType = 0;
    }
    lanType = '';
    v_pn = encodeURIComponent(v_pn);
    v_p = encodeURIComponent(v_p);
    v_kw = encodeURIComponent(v_kw);
    window.location = "/wt/meilishuo/web/index/webPosition210!getPostListByConditionShowPic?urlCorpEdition=null&operational=C10554B40A9913490E6688799F51EE30239DE608A027287038D73B95AD105EE05373F7BDEDCE01205B349F5841F267893E7A08DBD95D9A57F92C9A85E864EA4536C12B39795BD9EFF6EA8125AA8C82821A9C6EB914A72F2FFBCB3F12F44F03ECBAD2FFD09C6B9E4328E0741CAD4B299B"
            + "&positionType=" + v_pt
            + "&comPart=" + v_cp
            + "&sicCorpCode=" + v_scp
            + "&brandCode=" + brandCode
            + "&releaseTime=" + v_t
            + "&trademark=" + v_trademark
            + "&useForm=" + v_useForm
            + "&recruitType=" + v_recruitType
            + "&lanType=" + lanType
            + "&positionName=" + encodeURIComponent(v_pn)
            + "&workPlace=" + encodeURIComponent(v_p)
            + "&keyWord=" + encodeURIComponent(v_kw)
            + "&columnId=2";
}

function postNameOnFocus(obj){
   obj.value = "";
}

function postNameOnBulr(obj){
   if(obj.value==""){
     obj.value="请输入职位名称";
   }
}

function keyWordOnFocus(obj){
   obj.value = "";
}

function keyWordOnBlur(obj){
  if(obj.value==""){
       obj.value="请输入关键字";
     }
}

function chooseWorkPlaceAll(obj){
	jQuery(obj).parent().parent().children("li").attr("class","ml_1");
	jQuery(obj).parent().attr("class","all ml_1");
	//jQuery("#workPlaceNameV").val(0);
	jQuery("#workPlaceFirstLI").attr("class","work_place_title");
	searchPost();
}

function chooseWorkPlace(obj){
	jQuery(obj).parent().parent().children("li").attr("class","ml_1");
	jQuery("#workPlaceAll").attr("class","ml_1");
	jQuery(obj).parent().attr("class","all ml_1");
	jQuery("#workPlaceNameV").val(jQuery(obj).attr("data-value"));
	jQuery("#workPlaceFirstLI").attr("class","work_place_title");
	searchPost();
}

function choosePostTypeAll(obj){
	jQuery(obj).parent().parent().children("li").attr("class","ml_1");
	jQuery(obj).parent().attr("class","all ml_1");
	//jQuery("#positionTypeV").val(0);
	jQuery("#postTypeFirstLI").attr("class","work_place_title");
	searchPost();
}

function choosePostType(obj){
	jQuery(obj).parent().parent().children("li").attr("class","ml_1");
	jQuery("#postTypeAll").attr("class","ml_1");
	jQuery(obj).parent().attr("class","all ml_1");
	jQuery("#positionTypeV").val(jQuery(obj).attr("data-value"));
	jQuery("#postTypeFirstLI").attr("class","work_place_title");
	searchPost();
}

function chooseCompanyAll(obj){
	jQuery(obj).parent().parent().children("li").attr("class","ml_1");
	jQuery(obj).parent().attr("class","all ml_1");
	//jQuery("#comPartV").val(0);
	jQuery("#companyFirstLI").attr("class","work_place_title");
	searchPost();
}

function chooseCompany(obj){
	jQuery(obj).parent().parent().children("li").attr("class","ml_1");
	jQuery("#companyAll").attr("class","ml_1");
	jQuery(obj).parent().attr("class","all ml_1");
	jQuery("#comPartV").val(jQuery(obj).attr("data-value"));
	jQuery("#companyFirstLI").attr("class","work_place_title");
	searchPost();
}

function chooseCorpCode(obj){
	jQuery(obj).parent().parent().children("li").attr("class","ml_1");
	jQuery("#companyAll").attr("class","ml_1");
	jQuery(obj).parent().attr("class","all ml_1");
	jQuery("#comPartV").val(0);
	jQuery("#sicCorpCodeV").val(jQuery(obj).attr("data-value"));
	jQuery("#companyFirstLI").attr("class","work_place_title");
	searchPost();
}

</script>

<script type="text/javascript">
$(function(){
	
	var workPlaceNum=$("#workPlaceNum").val();
	var workTypeNum=$("#workTypeNum").val();
	var workCompanyNum=$("#workCompanyNum").val();
	  var $category=$('ul.work_place_area li:gt('+workPlaceNum+'):not(:last)');
	  $category.hide();
	  var $toggleBtn=$("li.showmore > a");
	  $toggleBtn.click(function(){
		 if($category.is(":visible")){
			 $category.hide();
			 $('.showmore a span').text("更多...");
		 } else{
			 $category.show();
			 $('.showmore a span').text("精简显示");
			 
		 }
	  });
	  var $categoryType=$('ul.work_type_area li:gt('+workTypeNum+'):not(:last)');
	  $categoryType.hide();
	  var $toggleBtnType=$("li.showmoreType > a");
	  $toggleBtnType.click(function(){
		 if($categoryType.is(":visible")){
			 $categoryType.hide();
			 $('.showmoreType a span').text("更多...");
		 } else{
			 $categoryType.show();
			 $('.showmoreType a span').text("精简显示");
			 
		 }
	  });
	  
	  var $categoryCompany=$('ul.work_company_area li:gt('+workCompanyNum+'):not(:last)');
	  $categoryCompany.hide();
	  var $toggleBtnCompany=$("li.showmoreCompany > a");
	  $toggleBtnCompany.click(function(){
		 if($categoryCompany.is(":visible")){
			 $categoryCompany.hide();
			 $('.showmoreCompany a span').text("更多...");
		 } else{
			 $categoryCompany.show();
			 $('.showmoreCompany a span').text("精简显示");
			 
		 }
	  });
});
</script>
