<script type="text/javascript">
    GJ.use('app/common/widget/widget.js', function (Widget) {
        Widget.initWidgets();
    });
    GJ.use('log_tracker', function(){
        var gjch = '/zhaopin/kuaizhaozhuanti/-/-/detail';
        var gjchNew = '/zhaopin/kuaizhaozhuanti/-/-/detail';
        GJ.LogTracker.gjch = gjch;
        GJ.LogTracker.gjchNew = gjchNew;
        GJ.LogTracker.trackPageView();
        GJ.LogTracker.bindAllTrackEvent();
    });

    GJ.use('jquery', function() {
        $('.bc-left').on('mouseenter', function() {
          $('.slide-prev,.slide-next').show();
        }).on('mouseleave', function() {
          $('.slide-prev,.slide-next').hide();
        });

        // 投递
        var applyUrl = $('.js-findjob-reply').attr('href');
        GJ.use('js/util/modal/modal.js, js/util/modal/modal.css,css/v5/job/job_modal.css', function(Modal) {
            var modal = new Modal({
                  $triggerEl: $(".js-findjob-reply"),
                  isScroll: false,
                  width : 620,
                  height: 370,
                  title: "申请职位",
                  url: applyUrl
            });
        });

        var $tabContainer = $('.tab');
        var tabTop = $tabContainer.offset().top;

        var $scrollTabList = $tabContainer.find('.fl a');
        var $scrollContentList = $('.part');
        var $placeholder = $('#placeholder');

        var topList = [];
        var TAB_COUNT = 6;
        var OFFSET = 50;
        var lastTab = 0;
        for (var i = 0; i < TAB_COUNT; i++) {
          var $content = $($scrollContentList[i]);
          topList[i] = $content.offset().top;
        }

        $scrollTabList.on('click', function() {
            var i = $(this).index();
            $('html, body').animate({
                'scrollTop': topList[i] - OFFSET
            });

            return false;
        });

        // scroll
        function onScroll() {
            var scrollTop = $(window).scrollTop();
            var currentTab = TAB_COUNT - 1;

            // 判断scrolltop在哪个位置
            if (scrollTop < tabTop) {
              $tabContainer.toggleClass('mt20', true).toggleClass('pos', false);
              $placeholder.hide();

              currentTab = 0;
            } else {
              $tabContainer.toggleClass('pos', true).toggleClass('mt20', false);
              $placeholder.show();

              for (var i = 0; i < TAB_COUNT - 1; i++) {
                if (scrollTop < topList[i + 1] - OFFSET - 20) {
                  currentTab = i;
                  break;
                }
              }
            }

            if (lastTab != currentTab) {
              $($scrollTabList[lastTab]).removeClass('act');
              $($scrollTabList[currentTab]).addClass('act');
              lastTab = currentTab;
            }
        }

        onScroll();
        $(window).on('scroll', onScroll);
    });

    var data = {
        sImgs : [],
        mImgs : [],
        bImgs : []
    };

    for (var i = 1; i <= 10; i++) {
      var prefix = 'http://k.ganjistatic1.com/app/blue/topic/images/fushikang/bworkimg/';
      data.sImgs.push(prefix  + i + '.jpg');
      data.mImgs.push(prefix  + i + '.jpg');
      data.bImgs.push(prefix  + i + '.jpg');
    }
    GJ.use('js/util/slideshow/slideshow.js', function(SlideShow){
        $('#my-slide').slideShow({
            start : 0,       // 起始图片的索引，从0开始

            interval : 5,    // 自动轮播的时间间隔，如果为0，表示不自动轮播，单位为秒

            repeat : true,  // 是否循环重复轮播

            width : 690,   // 组件宽度，如果为0，表示自由宽度

            viewHeight : 420,   // view视图高度

            navHeight : 53,     // navgation导航条高度

            arrWidth : 8,       // 导航箭头的宽度

            arrHeight : 20,     // 导航箭头的高度

            thumbWidth : 54,    // 缩略图的宽度

            space : 3,          // 缩略图的间距

            css : 'slideshow.css',     // css文件名

            eventType : 'click',      // 左右箭头的事件类型，支持hover和click

            floatBtn : true,   // view视图上的浮动按钮
            data : data
        });
    });
</script>
</body>
