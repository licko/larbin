<script>
  $('#headerNav').on('mouseenter', '#navDownLoad', function () {
    $('#mobileShow').slideDown();
  });
  $('#headerNav').on('mouseleave', '#navDownLoad', function () {
    $('#mobileShow').stop().slideUp();
  });
  $('.place-slide').on('mouseenter', function () {
    $('.pls-nav').slideDown();
  });
  $('.place-slide').on('mouseleave', function () {
    $('.pls-nav').stop().slideUp();
  });
	$('.navo').on('mouseover', function (event) {
	$(this).children("a").addClass("pls-navao");
	$(this).children('.navs-sl').slideDown();
  });
  $('.navo').on('mouseleave', function () {
	$(".navo").children("a").removeClass("pls-navao");
	$(this).children('.navs-sl').slideUp();
  });
</script>
