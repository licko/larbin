<script>
    $.post('/User/getpsid', function (json) {
        if($('#ifram-dev').length > 0) {
            var ifram = $('#ifram-dev');
            var url = 'http://visa.ilvxing.cn/common/syncses?psid=' + json;
        } else {
            var ifram = $('#ifram');
            var url = 'http://visa.ilvxing.com/common/syncses?psid=' + json;
        }
        var iframeHtml = document.createElement('iframe');
        iframeHtml.src = url;
        iframeHtml.style.display = 'none';
        ifram.append(iframeHtml);
    })
</script>
