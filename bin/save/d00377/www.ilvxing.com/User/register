<script type="text/javascript">
  $(function(){

//    $('label input').focusin(function() {
//      var input = $(this).parent().prev().find('input');
//      $(this).addClass('in');
//      //如果上一个input没有错误信息
//      if(!input.hasClass('error')) {
//        input.removeClass('in').addClass('success');
//
//      }
//      if(input.hasClass('valid')) {
//        if(input.next().find('label').length > 0){
//          input.next().find('label').removeClass('error').addClass('noerror').html('&nbsp;');
//        } else {
//          input.next().addClass('noerror').html('&nbsp;')
//        }
//      }
//    })





    $('label input').keyup(function() {
      var input =$(this);
      $(this).addClass('in');
      //如果上一个input没有错误信息
      if(input.attr('class')){
        if(input.attr('class').indexOf('valid') >= 0) {
          input.removeClass('in').addClass('success');
          input.parent().next().addClass('noerror').empty();
        }
        if(input.attr('class').indexOf('error') >= 0) {
          if(input.next().find('label').length > 0){
            input.parent().next().removeClass('noerror');
            input.parent().next().find('label').removeClass('noerror');
          } else {
            input.parent().next().removeClass('noerror')
          }
        }
      }


    })

    $('label input').focusin(function() {
      var input =$(this).parent().parent().prev().find('input');
      $(this).addClass('in');
      //如果上一个input没有错误信息
      if(input.attr('class')){
        if(input.attr('class').indexOf('valid') >= 0) {
          input.removeClass('in').addClass('success');
          input.parent().next().addClass('noerror').empty();
        }
        if(input.attr('class').indexOf('error') >= 0) {
          if(input.next().find('label').length > 0){
            input.parent().next().removeClass('noerror');
            input.parent().next().find('label').removeClass('noerror');
          } else {
            input.parent().next().removeClass('noerror')
          }
        }
      }


    })


    //清除icon
    $('#reset').click(function() {
      $('label').removeClass('noerror')
    });
    var form = $("#regForm");
    $.validator.addMethod("isLegalAccount", function(value, element) {
      return (/^1[0-9]{10}$/).test(value) || $.validator.methods.email.call(this,value,element);
    }, "请输入手机号或者邮箱");

    //添加验证用户名是否存在方法
    /*$.validator.addMethod("isAccountNotExist", function(value, element) {
     var retVal = true;
     //同步验证用户名
     //  console.log("ajaxchecking...");
     $.ajax({
     url:'/User/checkUser',
     data:{account:value},
     success:function(ret){
     retVal = ret.ok;
     },
     dataType:'json',
     async:false
     });
     return retVal;
     }, "用户名已经存在");
     //添加验证昵称是否存在方法
     $.validator.addMethod("isNicknameNotExist", function(value, element) {
     var retVal = true;
     //同步验证用户名
     //  console.log("ajaxchecking...");
     $.ajax({
     url:'/User/checkNickname',
     data:{nickname:value},
     success:function(ret){
     retVal = ret.ok;
     },
     dataType:'json',
     async:false
     });
     return retVal;
     }, "该昵称已经被占用");*/

    form.validate({
      rules:{
        account:{required:true,isLegalAccount: true,
          remote:{
            type:"POST",
            url:"/User/checkUser",
            data:{
              account:function(){return $("#account").val();}
            }
          }
        },
        password:{required:true,minlength:6},
        rePassword:{equalTo:"#password"},
        verify:{required:true},
        agree:{required:true}
      },



      messages:{
        account:{required:"请输入注册账号",remote:"账号已经存在"},
        password:{required:"请设置您的密码",minlength:"密码长度不够"},
        rePassword:{equalTo:"两次密码不一致"},
        verify:{required:"请输入验证码"},
        agree:{required:"您没有同意服务协议"}
      },
      errorPlacement: function(error, element) {
        if(element.parent().parent().find('span').hasClass('noerror')){
          element.parent().parent().find('span').removeClass('noerror');
        }
          element.parent().parent().find('span').append(error);


        if (element.attr('class').indexOf('error') >= 0) {
          if (element.next().find('label').length > 0) {
            element.next().removeClass('noerror');
            element.next().find('label').removeClass('noerror');
          } else {
            element.next().removeClass('noerror')
          }
        }



      },
      submitHandler:function() {
        //防止多次提交
        if(form.data("ajaxing") === 'true') return false;
        form.data("ajaxing",true);
        //$.post(url,data,callback,dataType)
        $.post('/User/addUser',form.serialize(),function(ret){
          form.data("ajaxing",false);
          if(ret.ok){
            window.location.href = "/User/verify_user";
          } else {
            $.popup.alert(ret.msg);
            return false;
          }
        },'json');
        //永远return false,表单用异步来提交
        return false;
      }
    });
    function changeVerify(){
      var timenow = new Date().getTime();
      document.getElementById('verifyImg').src='/User/verify/'+timenow;
    };

    $("#login_section>a").click(function(){
      var type = $(this).data("type");
      if(type){
        window.location.href = ''+BASE_URL+'/login_api/'+type+'/?callback='+(BASE_URL+'/ThirdParty/login');
        return false;
      }

    });



  });
</script>

<script language="JavaScript">
    function changeVerify(){
        var timenow = new Date().getTime();
        document.getElementById('verifyImg').src='/User/verify/'+timenow;
    }
</script>

