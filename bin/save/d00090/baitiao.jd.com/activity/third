<script>
        pageUl();
        function pageUl(){
            var oPage=$('#page-ul li');
            oPage.eq(0).addClass('current');
            var pageCount=oPage.length;
            for(var i=0;i<oPage.length;i++){
                var pageCon=i+1+ ' / '+ pageCount;
                $('.num-font').eq(i).html(pageCon);
            }
        }

    var baitiao_tj_serverDomains = baitiao_ref_gethttphead_tj() + "://jrrem.jd.com/baitiao-ref";
    var url = null;
//    jQuery.each(baitiaoInfoList, function(i,item){
//        alert("sdd");
//      alert(item.id+","+item.skuId);
//   });
    var skuIds = [];
    for(var j=1;j<=4;j++) {
        var skuId = jQuery("#sku" + j).val();
        skuIds.push(skuId);
    }
    if(baitiao_ref_gethttphead_tj() =="http"){
        url = 'http://p.3.cn/prices/mgets?skuIds=J_' + skuIds.join(',J_') + '&type=1';
    }
    else{
        url = baitiao_tj_serverDomains+'/prices/mgets?skuIds=J_' + skuIds.join(',J_') + '&type=1';
    }
    jQuery.ajax({
        url: url,
        dataType: 'jsonp',
        success: function (r) {
            if (!r && !r.length) {
                return false;
            }
            for (var i = 0; i < r.length; i++) {
                var sku = r[i].id.replace('J_', '');
                var price = parseFloat(r[i].p, 10);

                if (price > 0) {
                    var price = r[i].p;
                    price =((price/12)*1.06).toFixed(2);
                    $("#fenqiPrice" + sku).html('<em class="em-1">￥</em>' + price + '&nbsp;<em class="em-2">X</em>&nbsp;12<em class="em-2">期</em>');

                } else {
                    $("#fenqiPrice" + sku).html('暂无报价');
                }
            }
        }
    });



    function baitiao_ref_gethttphead_tj(){
        var url = window.location.href,
                protocol = window.location.protocol;

        if (protocol.indexOf('https') < 0 ) {
            return 'http';
        }
        else {
            return 'https';
        }
    }
