        t = $('#pro_con').offset().top;
        mh = $('#pro_left').height();
        fh = $('#pro_con').height();
        $(window).scroll(function(e){
            s = $(document).scrollTop();
            if(s > t - 10){
                $('#pro_con').css('position','fixed');
                $('#gotop').show();
                if(s + fh > mh){
                    $('#pro_con').css('top',mh-s-fh+'px');
                }
            }else{
                $('#pro_con').css('top','0');
                $('#pro_con').css('position','');
                $('#gotop').hide();
            }

        })
