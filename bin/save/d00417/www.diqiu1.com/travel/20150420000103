                var pidval = $('.hyj5').text() * 1.06 / 12;
                $($lid).html('<img src="/resources/pc/images/lybt.jpg" class="img1">');//分期logo
                $($nid).text(12);//期数
                $($iid).text(pidval.toFixed(2));//分期价
                $("#installmentPrice").text(pidval.toFixed(2));
                $("#jdFinance_name").text("旅游白条：");
                $("#jdFinance_stage").text("12");
            }
            if ($ijd.find('.installmentMark').val() == '10') {
                var pidval = $('.hyj5').text() / 10;
                $($lid).html('<img src="/resources/pc/images/oneoverten.jpg" class="img1">');//分期logo
                $($nid).text(10);//期数
                $($iid).text(pidval.toFixed(2));//分期价
                $("#installmentPrice").text(pidval.toFixed(2));
                $("#jdFinance_name").text("分期价：");
                $("#jdFinance_stage").text("10");
