    //验证出行日期，弹出登录框让会员登录
    $("#btn_ljyd").click(function() {
        if ($("#tripdate").val()==""){
            $("html,body").stop().animate({scrollTop:$("#myModal").offset().top},1000);//滚动到左侧出行日期的位置
            shake($("#cfrq"),"rq",3);//右侧出行日期闪动
            return false;
        }else{//弹出登录框
            $.ajax({
                type:'POST',
                url:'/product/login',
                success:function(data){
                    if(data==false) {
                       // var redirectUrl = "/order/orderinit?pid=752&pdid="+ $('#pdid').val()+"";
                       	 $("#pnum").val($("#chengrennum").val());
                         var url = "/order/orderinit?"+$('#orderInit').serialize();
       					 var redirectUrl = encodeURIComponent(url);
                        $("#redirect").val(redirectUrl)
                        $("#overlay").show();
                        $("#login-popup").fadeIn();

                        var windowWidth = document.documentElement.clientWidth;
                        var windowHeight = document.documentElement.clientHeight;
                        var popupHeight = $("#login-popup").height();
                        var popupWidth = $("#login-popup").width();
                        $("#login-popup").css({
                            "position": "fixed",
                            "top": windowHeight / 2 - popupHeight / 2,
                            "left": windowWidth / 2 - popupWidth / 2,
                            "z-index": 2100
                        });

                        $("#overlay").click(function() {
                            $("#login-popup").fadeOut();
                            $("#overlay").hide();
                        });

                        $("#login-popup").find(".close1").click(function() {
                            $("#login-popup").fadeOut();
                            $("#overlay").hide();
                        });
                    } else {
                        $.ajax({
                            type:'POST',
                            url:'/product/getProduct',
                            data:{"sn":"zh20150309000005"},
                            success:function(data) {
                                var status = data.check;
                                if(status!="0") {
                                    $("#btn_ljyd").removeClass().addClass("btn btn-apply");
                                    $("#btn_ljyd").val("已结束");
                                    $("#btn_ljyd").attr('disabled',"true");
                                } else {






                                    var pdid = $('#pdid').val();
                                    $("#pdid").val(pdid);
                                    $("#pnum").val($("#chengrennum").val());
                                    
                                    $("#orderInit").submit();
                                    //window.location.href="/order/orderinit?pid=752&pdid="+pdid+"";
                                }
                            }
                        });
                    }
                }
            });

        }
    });

    //弹出框注册跳转
    function redirectReg(redirect) {
 //       var redirectUrl = "/order/orderinit?pid=752&pdid="+ $('#pdid').val()+"";
 		 $("#pnum").val($("#chengrennum").val());
        var url = "/order/orderinit?"+$('#orderInit').serialize();
        var redirectUri = encodeURIComponent(encodeURIComponent(url));
        window.location.href="/iproduct/"+redirect+"?url="+redirectUri+"";
    }

    //计时器js
    updateEndTime();

    //倒计时函数
    function updateEndTime()
    {
        $(".timer").each(function(i){
            var lag =parseInt(this.getAttribute("data-num"));
            $(this).attr("data-num",--lag);
            if(lag > 0)
            {
                var minite = Math.floor((lag / 60) % 60);
                var hour = Math.floor((lag / 3600) % 24);
                var day = Math.floor((lag / 3600) / 24);
                var second=returnFloat1(lag-day*3600*24-hour*3600-minite*60);
                $(this).children("cite").html(day+"天"+hour+"小时"+minite+"分"+second+"秒");
            }
            else
                $(this).children("cite").html("抢光啦");
        });
        setTimeout("updateEndTime()",100);
    }
    function returnFloat1(value){
        value = Math.round(parseFloat(value) * 10) / 10;
        if (value.toString().indexOf(".") < 0)
            value = value.toString() + ".0";
        return value;
    }

    //日期为空时闪动函数
    function shake(ele,cls,times){
        var i = 0,t= false ,o =ele.attr("class")+" ",c ="",times=times||2;
        if(t) return;
        t= setInterval(function(){
            i++;
            c = i%2 ? o+cls : o;
            ele.attr("class",c);
            if(i==3*times){
                clearInterval(t);
                ele.removeClass(cls);
            }
        },200);
    }

    //登录验证
    $("#loginbtn").click(function() {

        var $loginpopupform = $("#login-popup-form");
        $loginpopupform.validate({
            errorElement:"font",
            rules: {
                account: {//邮箱
                    required: true,
                },
                password: {//密码
                    required: true,
                },
                captcha: {//验证码
                    required: true,
                },
            },
            errorPlacement: function(error, element) {
                error.appendTo(element.parent());
            }
        });
    });

