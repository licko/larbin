<script type="text/javascript">

    $(document).ready(function(e) {
    
    $("#additionalTable").hide(); //隐藏可附加服务 table
    $("#tabs_kfjfw").hide(); //可附加服务选项卡
    var endstr = $("#mdd").text();
    if(endstr.length>9) {
    	endstr = endstr.substring(0,9)+"...";
    	$("#mdd").text(endstr)
    } 
    
    if(isClick==0) {
    	$(".spinnerExample2").parent().find('.increase').attr('disabled','disabled');
    }

	//可附加服务使用日期
		$('.syrq').datepicker();
		$('.syrq').val(formatterDate(new Date()));

        $("#curzhuti").removeClass().addClass("cur");

        //右侧产品详情滚动监听
        t = $('#pro_con').offset().top;
        mh = $('#pro_left').height();
        fh = $('#pro_con').height();
        $(window).scroll(function(e){
            s = $(document).scrollTop();
            if(s > t - 10){
                $('#pro_con').css('position','fixed');
                $('#gotop').show();
                if(s + fh > mh){
                    $('#pro_con').css('top',mh-s-fh+'px');
                }
            }else{
                $('#pro_con').css('top','0');
                $('#pro_con').css('position','');
                $('#gotop').hide();
            }

        })
