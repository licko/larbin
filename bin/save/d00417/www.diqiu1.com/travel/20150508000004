        //推荐产品轮播
        $(function(){


            //初始化，自动计算分期付款，分期价格＝订单金额 * 1.06 / 12，保留两位小数。
            var $ijd = $('.tour-showcase');
            var $iid = $ijd.find(".installmentPrice");//分期价
            var $lid = $ijd.find(".installmentLogo");//分期logo
            var $nid = $ijd.find(".installmentNum");//期数
            if ($ijd.find('.installmentMark').val() == '12') {
                var pidval = $('.hyj5').text() * 1.06 / 12;
                $($lid).html('<img src="/resources/pc/images/lybt.jpg" class="img1">');//分期logo
                $($nid).text(12);//期数
                $($iid).text(pidval.toFixed(2));//分期价
                $("#installmentPrice").text(pidval.toFixed(2));
                $("#jdFinance_name").text("旅游白条：");
                $("#jdFinance_stage").text("12");
            }
            if ($ijd.find('.installmentMark').val() == '10') {
                var pidval = $('.hyj5').text() / 10;
                $($lid).html('<img src="/resources/pc/images/oneoverten.jpg" class="img1">');//分期logo
                $($nid).text(10);//期数
                $($iid).text(pidval.toFixed(2));//分期价
                $("#installmentPrice").text(pidval.toFixed(2));
                $("#jdFinance_name").text("分期价：");
                $("#jdFinance_stage").text("10");
            }


            $.ajax({
                url: "/commendationpro/getrecommendationproductlist.do?id="+1094,
                type: "POST",
                dataType: "json",
                cache: false,
                success: function(data) {
                    //var a=JSON.stringify(data);
                    //alert(a);
