        t = $('#pro_con').offset().top;
        mh = $('#pro_left').height();
        fh = $('#pro_con').height();
        $(window).scroll(function(e){
            s = $(document).scrollTop();
            if(s > t - 10){
                $('#pro_con').css('position','fixed');
                $('#gotop').show();
                if(s + fh > mh){
                    $('#pro_con').css('top',mh-s-fh+'px');
                }
            }else{
                $('#pro_con').css('top','0');
                $('#pro_con').css('position','');
                $('#gotop').hide();
            }

        })
        //右侧微信和返回顶部浮动框
        $('#gotop').click(function(){
            $("html,body").stop().animate({scrollTop:0},500);
        });
        $('#code').hover(function(){
            $(this).attr('id','code_hover');
            $('#code_img').show();
        },function(){
            $(this).attr('id','code');
            $('#code_img').hide();
        })

        //起价说明提示框
