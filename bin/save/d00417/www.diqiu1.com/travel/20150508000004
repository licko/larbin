                            var html='<li><div class="img"><a href="/travel/'+sn+'" target="_blank" ><img class="lazy" src="http://diqiu1-img.oss-cn-beijing.aliyuncs.com/'+img+'"/><span>'+brief+'</span></a></div> </li>';
                            $("ul.slides").append(html);


                        }
                        $('.flexslider').show().flexslider({
                            animation: "slide",
                            start: function(slider){
                                $('body').removeClass('loading');
                            }
                        });

                    }
                }
            });

        });

    });

