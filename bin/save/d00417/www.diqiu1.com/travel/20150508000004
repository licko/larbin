            return false;
        }else{//弹出登录框
            $.ajax({
                type:'POST',
                url:'/product/login',
                success:function(data){
                    if(data==false) {
                       // var redirectUrl = "/order/orderinit?pid=1094&pdid="+ $('#pdid').val()+"";
