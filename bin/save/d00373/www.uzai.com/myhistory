    <script type="text/javascript">
        function clearAllScan() {
            if (confirm("确定要清空全部浏览记录？")) {
                if (uzaiGlobalConfig.userID) {
                    if (location.href.indexOf("sh.uzai.com") > -1 || location.href.indexOf("www.uzai.com") > -1 || location.href.indexOf("bj.uzai.com") > -1) {
                        $.get("/ScanRecord/Delete/?rand=" + Math.random(), { id: -2, uid: uzaiGlobalConfig.userID }, function (data) {
                            $("#rs-browseMsg").remove();
                            $("#rs-browseBot").remove();
                            $("#rs-browseTop").after(data);
                            window.location.reload();
                        });

                    }
                } else {
                    clearscan("", true);
                    window.location.reload();
                }

            }
        }
        function cutCopPage(ele, obj) {
            var o = $(ele);
            var op = o.parent('div');
            var os = o.siblings('a');
            os.removeClass('current');
            o.addClass('current');
            $('#historyproduct .itemlist').empty();
            if (location.href.indexOf("sh.uzai.com") > -1 || location.href.indexOf("www.uzai.com") > -1 || location.href.indexOf("bj.uzai.com") > -1) {
                $.ajax({ url: "/HistoryScan/GetHistoryProduct/?rand=" + Math.random(),
                    type: "GET",
                    dataType: "json",
                    data: { pageIndex: obj, uid: uzaiGlobalConfig.userID },
                    success: function (result) {
                        $('#historyproduct').empty();
                        var s = result;
                        var sub = "";
                        if (s.HistoryScanProduct != null) {
                            var index = 0;
                            for (var i in s.HistoryScanProduct) {

                                var fg = i;

                                var ilist = "<div class='itemlist'>";
                                var item = "";

                                var flag = "";

                                if (fg == "0")
                                    flag = "today";
                                else if (fg == '1')
                                    flag = "yestoday";
                                else
                                    flag = "more";

                                for (var m = 0; m < s.HistoryScanProduct[i].length; m++) {
                                    if (m == 0)
                                        flag = "<i class='date-" + flag + "' ></i>";
                                    else
                                        flag = "<i class='date-no'></i>";

                                    var j = s.HistoryScanProduct[i][m];
                                    item += "<div class='item cfix'><div class='ldate'>" + flag + "<i class='date-time'><span>" + j.Date + "</span></i></div><div class='riteminfo fn-left'><div class='fn-left img'><a href='" + j.ProductURL + "' target='_blank'><img width='120'height='75'src='" + j.ProductPicURL + "'alt=''></a></div><dl class='dl fn-left'><dt class='t'><a href='" + j.ProductURL + "'  target='_blank'>" + j.ProductName + "</a></dt><dd class='desc'>" + j.ProdcutNameExtension + "</dd></dl><div class='item-price fn-left'><p><span class='unit'>￥</span><span class='num'>" + j.ProductPrice + "</span><span>起</span></p><p><a href='" + j.ProductURL + "'  target='_blank' class='fn-btn fn-yelbtn3'><span>查看详情</span></a></p></div></div></div>";
                                }

                                sub += (ilist + item + "</div>");
                                index += 1;

                            }


                            $('#historyproduct').append(sub);

                        }
                    }


                });
            }


        }
    </script>
